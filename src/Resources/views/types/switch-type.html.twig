{% block runopencode_switch_type_widget %}

    <runopencode-switch-type
        {% for key, value in form.vars.configuration %}
            {{ key }}="{{ value }}"
        {% endfor %}
        {{ block('widget_attributes') }}
    >
        {{ form_widget(form) }}
    </runopencode-switch-type>

{% endblock runopencode_switch_type_widget %}
